{
  "name": "electron-app",
  "version": "4.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "Tohrusky",
  "homepage": "https://github.com/Tohrusky",
  "scripts": {
    "dev": "electron-vite dev",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:mac-arm64": "npm run build && electron-builder --mac --arm64",
    "build:mac-x64": "npm run build && electron-builder --mac --x64",
    "build:mac": "npm run build && electron-builder --mac --universal",
    "build:win": "npm run build && electron-builder --win --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@vicons/antd": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "core-js": "^3.29.0",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.32",
    "pinia-plugin-persistedstate": "^3.1.0",
    "sass": "^1.59.3",
    "systeminformation": "^5.17.12",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.3",
    "vue": "^3.2.47",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^1.0.2",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.18.14",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "electron": "^22.3.2",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.20",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "prettier": "^2.8.4"
  }
}
